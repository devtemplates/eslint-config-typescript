    "eslint-plugin-tsdoc": "0.2.17"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.2",
    "@commitlint/config-conventional": "18.4.2",
    "@commitlint/cz-commitlint": "18.4.2",
    "commitizen": "4.3.0",
    "eslint": "8.53.0",
    "husky": "8.0.3",
    "inquirer": "8.2.6",
